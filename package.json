{
  "name": "@codebyjordan/ui-demo",
  "description": "Docs & Demo App for @codebyjordan/ui",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "fix-memory": "cross-env LIMIT=4096 increase-memory-limit",
    "build:app": "rm -rf docs/@codebyjordan/* && cp -r dist/ui docs/@codebyjordan/ui && ng build --prod",
    "build:lib": "ng-packagr && scss-bundle -c scss-bundle-ui.config.json && scss-bundle -c scss-bundle-bootstrap.config.json && scss-bundle -c scss-bundle-core.config.json && scss-bundle -c scss-bundle-vars.config.json && scss-bundle -c scss-bundle-mixins.config.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^5.2.9",
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/forms": "^5.2.9",
    "@angular/http": "^5.2.9",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "@angular/router": "^5.2.9",
    "bootstrap": "^4.0.0",
    "core-js": "^2.5.3",
    "prismjs": "^1.12.2",
    "rxjs": "^5.5.7",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.3",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/language-service": "^5.2.9",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.102",
    "@types/prismjs": "^1.9.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-node": "^5.1.1",
    "codelyzer": "~3.2.0",
    "cross-env": "^5.1.4",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^1.7.0",
    "protractor": "~5.1.2",
    "rimraf": "^2.6.1",
    "scss-bundle": "^2.1.3",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": ">=2.4.2 <2.5.0"
  }
}
